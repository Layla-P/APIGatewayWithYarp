{
  "Logging": {
	"LogLevel": {
	  "Default": "Information",
	  "Microsoft": "Warning",
	  "Microsoft.Hosting.Lifetime": "Information"
	}
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
	"Routes": {
	  "food-route": {
		"ClusterId": "food_cluster",
		"Match": {
		  "Path": "foodservice/{**catchall}"
		},
		"Transforms": [
		  {
			"PathPattern": "{**catchall}"
		  }
		]
	  },
	  "drink-route": {
		"ClusterId": "drink_cluster",
		"Match": {
		  "Path": "drinkservice/{**catchall}"
		},
		"Transforms": [
		  {
			"PathPattern": "{**catchall}"
		  }
		]
	  }
	},
	"Clusters": {
	  "food_cluster": {
		"Destinations": {
		  "destination1": {
			"Address": "https://localhost:44378/"
		  }
		}
	  },
	  "drink_cluster": {
		"Destinations": {
		  "destination1": {
			"Address": "https://localhost:44366/"
		  }
		}
	  }
	}
  }

}
